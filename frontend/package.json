{
  "name": "poa-poba-frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/api": {
      "target": "http://localhost:3001"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "font-awesome": "^4.7.0",
    "glamor": "^2.20.40",
    "glamorous": "^4.12.5",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-plaid-link": "^1.0.2",
    "react-scripts": "^1.1.4",
    "sweetalert2": "^7.19.3",
    "truffle-contract": "^3.0.5",
    "web3": "^1.0.0-beta.34"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint ."
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "prettier": "^1.12.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/ui/**/*.js",
      "**/src/*.js",
      "!**/build/**",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/public/**",
      "!**/artifacts/**",
      "!**/__tests__/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 55,
        "functions": 55,
        "lines": 55,
        "statements": 55
      }
    }
  }
}
