{
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "func-names": "off",
    "no-console": "off",
    "prefer-template": "off",
    "arrow-body-style": "off"
  },
  "globals": {
    "jest": false,
    "describe": false,
    "it": false,
    "expect": false
  }
}
